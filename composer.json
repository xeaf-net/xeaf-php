        "ext-pdo"       : "*",
        "ext-pdo_sqlite": "*",
        "ext-pdo_mysql" : "*",
        "ext-pdo_pgsql" : "*",
        "ext-redis"     : "*",
        "ext-imagick"   : "*",
        "ext-zip"       : "*",
